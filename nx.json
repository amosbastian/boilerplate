{
  "npmScope": "boilerplate",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "build-storybook"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "blog": {
      "tags": []
    },
    "blog-e2e": {
      "tags": [],
      "implicitDependencies": ["blog"]
    },
    "blog-i18n": {
      "tags": []
    },
    "shared-i18n": {
      "tags": []
    },
    "shared-storybook": {
      "tags": []
    },
    "shared-theme": {
      "tags": []
    },
    "shared-types": {
      "tags": []
    },
    "shared-ui": {
      "tags": []
    },
    "shared-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["shared-ui"]
    },
    "site": {
      "tags": []
    },
    "site-e2e": {
      "tags": [],
      "implicitDependencies": ["site"]
    },
    "site-i18n": {
      "tags": []
    },
    "site-ui": {
      "tags": []
    },
    "site-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["site-ui"]
    },
    "stripe": {
      "tags": []
    }
  }
}
